{
  "name": "t3000-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "private": true,
  "description": "T3000 API, a storage of building dbs and hvac applications",
  "scripts": {
    "start": "NODE_ENV=production node -r dotenv/config dist/server",
    "dev": "nodemon --ext ts --exec \"npm -s run compile && NODE_ENV=development node -r dotenv/config dist/server\"",
    "build": "npm -s run clean && npm -s run compile",
    "clean": "rm -rf dist",
    "compile": "swc src -d dist --config-file swcrc.json",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:db-push": "prisma db push",
    "prisma:db-pull": "prisma db pull",
    "prettier": "prettier --write src",
    "docs:dev": "vuepress dev docs --clean-cache",
    "docs:build": "vuepress build docs",
    "rename:inputs": "replace-in-file ** --configFile=replace-config.js"
  },
  "dependencies": {
    "@fastify/cookie": "^8.1.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/multipart": "^7.1.0",
    "@paljs/plugins": "^5.2.0",
    "@prisma/client": "^4.10.1",
    "change-case": "^4.1.2",
    "dotenv": "^16.0.0",
    "fastify": "^4.2.0",
    "flat": "^5.0.2",
    "graphql": "^16.5.0",
    "graphql-middleware": "^6.1.24",
    "graphql-scalars": "^1.17.0",
    "jsonwebtoken": "^9.0.0",
    "mercurius": "^12.2.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.6",
    "pluralize": "^8.0.0",
    "sharp": "^0.31.0"
  },
  "devDependencies": {
    "@prisma/internals": "^4.10.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.209",
    "@types/node": "^18.7.15",
    "@vuepress/cli": "^2.0.0-beta.33",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.33",
    "@vuepress/plugin-search": "^2.0.0-beta.33",
    "@vuepress/plugin-shiki": "^2.0.0-beta.33",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "prisma": "^4.2.1",
    "replace-in-file": "^6.3.2",
    "typescript": "^4.5.4",
    "vuepress": "^2.0.0-beta.60"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "repository": "",
  "author": "Temco Controls"
}
